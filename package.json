{
  "name": "dynamic-material-ui",
  "description": "Material UI based dynamic form component for React using JSON-Schema",
  "author": "Dinakaran S",
  "user": "dinakarans",
  "version": "1.3.0-beta",
  "scripts": {
    "start": "webpack-dev-server --port 8000",
    "test": "jest --",
    "test:coverage": "jest --coverage --",
    "test:watch": "jest --watch --",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache",
    "gh-pages": "webpack",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --profile --json > stats.json",
    "dist": "webpack",
    "dist:min": "webpack",
    "dist:modules": "rimraf ./dist-modules && babel ./src --out-dir ./dist-modules",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node lib/post_install.js"
  },
  "main": "dist-modules",
  "module": "src",
  "jsnext:main": "src",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@date-io/date-fns": "^1.3.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react-hmre": "^1.1.1",
    "catalog": "^3.6.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "date-fns": "^2.0.0-alpha.27",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "git-prepush-hook": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^24.8.0",
    "jquery": "^3.4.1",
    "json-loader": "^0.5.7",
    "material-ui-pickers": "^2.2.4",
    "mini-css-extract-plugin": "^0.6.0",
    "npm": "^6.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.7.2",
    "purecss": "^1.0.0",
    "raw-loader": "^1.0.0",
    "react": "^16.8.6",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-github-corner": "^2.3.0",
    "react-highlight": "^0.12.0",
    "react-json-tree": "^0.11.2",
    "react-split-pane": "^0.1.87",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "sync-exec": "^0.6.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "react": ">=16.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DinakaranS/dynamic-material-ui.git"
  },
  "homepage": "https://github.com/DinakaranS/dynamic-material-ui",
  "bugs": {
    "url": "https://github.com/DinakaranS/dynamic-material-ui/issues"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "dynamic-forms",
    "dynamic forms",
    "playground",
    "forms",
    "build interactive form",
    "robo-form",
    "@material-ui/core"
  ],
  "license": "MIT",
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react-flexbox-grid": "^2.1.2",
    "react-select": "^2.4.3",
    "react-simple-tooltip": "^2.5.0",
    "validator": "^10.11.0"
  }
}
